name: ShipTrajectoryPrediction

conda_env: conda.yaml

entry_points:
  download_data:
    command: "python data_download.py"

  preprocess_data:
    command: "python data_preprocess.py"

  train:
    parameters:
      arch: {type: str, default: "all"}
    command: "python train.py --arch {arch}"

  evaluate:
    command: "python evaluate.py"
